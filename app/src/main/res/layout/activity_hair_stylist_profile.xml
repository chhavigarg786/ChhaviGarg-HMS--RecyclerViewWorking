<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HairStylistProfile">

    <ImageView
        android:id="@+id/hairStylistImageView"
        android:layout_width="201dp"
        android:layout_height="0dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="31dp"
        app:layout_constraintBottom_toTopOf="@+id/hairStylistName3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/hms_logo2" />

    <TextView
        android:id="@+id/hairStylistRating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="39dp"
        android:layout_marginBottom="187dp"
        android:text="HairStylistRating"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintStart_toStartOf="@+id/hairStylistImageView"
        app:layout_constraintTop_toBottomOf="@+id/hairStylistName3" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hairStylistRating"
        app:menu="@menu/menu" />

    <TextView
        android:id="@+id/hairStylistName3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="40dp"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/hairStylistRating"
        app:layout_constraintStart_toStartOf="@+id/hairStylistRating"
        app:layout_constraintTop_toBottomOf="@+id/hairStylistImageView" />
</androidx.constraintlayout.widget.ConstraintLayout>